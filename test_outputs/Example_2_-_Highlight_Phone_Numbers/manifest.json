{
  "manifest_version": 3,
  "name": "Generated Extension",
  "version": "1.0",
  "description": "Make an extension that highlights all phone numbers on any website.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}